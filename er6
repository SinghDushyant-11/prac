jenkinse>new item>p6>pipeline>

script 

pipeline
{
    agent any
    stages
    {
        stage('Checkout')
        {
            steps
            {
                git branch:'main',url:'https://github.com/AnnaBinoy09/demo_devops.git' // change to your repository path
            }
        }
        stage('Build')
        {
            steps
            {
                sh 'mvn clean package'
            }
        }
        stage('Test')
        {
            steps
            {
                sh 'mvn test'
            }
        }
        
    }
}

>save>build

p6>config if mistake









